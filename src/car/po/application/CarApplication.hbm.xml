<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="car.po.application.CarApplication" table="carapplication">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="usingDate" column="usingdate"/>
        <property name="destination" column="destination"/>
        <property name="workID" column="affairsid"/>
        <property name="status" column="status"/>
        <property name="remarks" column="remarks"/>
        <property name="userID" column="userid"/>
        <property name="userName" column="username"/>
        <property name="carID" column="carid"/>
        <property name="handler" column="adminid"/>
        <property name="applicationDate" column="date"/>
        <property type="java.lang.Double" name="longitude" column="longitude"/>
        <property type="java.lang.Double" name="latitude"  column="latitude"/>
    </class>
</hibernate-mapping>