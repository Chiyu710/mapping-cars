<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="car.po.application.FixApplication" table="fixapplication">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="licensePlate" column="LP"/>
        <property name="mileage" column="mileage"/>
        <property name="remarks" column="remarks"/>
        <property name="type" column="type"/>
        <property name="userID" column="userid"/>
        <property name="userName" column="username"/>
        <property name="carID" column="carid"/>
        <property name="handler" column="adminid"/>
        <property name="applicationDate" column="date"/>
        <property name="status" column="status"/>
        <property name="location" column="location"/>
    </class>
</hibernate-mapping>