<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="car.po.record.StatusLog" table="statuslog">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="staffId" column="staffId"/>
        <property name="oldStatus" column="oldStatus"/>
        <property name="newStatus" column="newStatus"/>
        <property name="remarks" column="remarks"/>
        <property name="changeTime" column="changeTime"/>
    </class>
</hibernate-mapping>