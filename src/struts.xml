<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="User" namespace="/" extends="struts-default">
        <action name="login" class="userAction" method="login">
            <result name="success">staff_index.jsp</result>
            <result name="fail">login.jsp</result>
        </action>

        <action name="register" class="userAction" method="register">
            <result name="success">suuccess.jsp</result>
            <result name="fail">Failed.jsp</result>
        </action>
    </package>

    <package name="Application" namespace="/" extends="struts-default">
        <action name="sendFix" class="applicationAction" method="sendFix">
            <result name="success">../view_auth/staff_index.jsp</result>
            <result name="fail">../view_auth/Failed.jsp</result>
        </action>
    </package>
</struts>